<nav aria-label="breadcrumb">
	<ol class="breadcrumb">
		<li class="breadcrumb-item"><a href="/">Home</a></li>
		<li class="breadcrumb-item"><a href="/">Blog</a></li>
		<li class="breadcrumb-item active" aria-current="page">{{data.post.title}}</li>
	</ol>
</nav>

<section id="#blog" class="py-5">
	<div class="container blog-blogpage">
		<div class="row">

			<div class="col-lg-8">
				<div class="blog-post-full">
					<div class="blog-post-heading">
						<h3>{{data.post.title}}</h3>
					</div>
					<div class="blog-post-info row">
						<div class="blog-post-info-avatar col-2">
							<img src="/images/blogin/writer.png" alt="avatar">
						</div>
						<div class="blog-post-info-detail col-12 col-lg-3">
							<p class="blog-post-info-detail-author">BY {{data.post.author.name.first}} {{data.post.author.name.last}}</p>
							<p class="blog-post-info-detail-time">{{date data.post.publishedDate}}</p>
						</div>
						<div class="blog-post-info-detail col-4 col-lg-3">
							<a href="{{categoryUrl data.post.categories.0.name}}" class="blog-item-tag">{{data.post.categories.0.name}}</a>
						</div>
					</div>

					<div class="blog-post-content">
						{{{data.post.content.extended}}}
					</div>
				</div>

        <div class="blog-post-tag--end my-4">
          {{#each data.post.categories}}
            <a href="{{categoryUrl name}}"  class="blog-item-tag mr-3">{{name}}</a>
          {{/each}}
        </div>

			</div>


			<div class="col-lg-4">

				<div class="blog-recent-posts mb-4">
					<h3>Recent Posts</h3>
					<div class="blog-recent-posts-link">
						<a href="#">6 Powerful Tips To Creating Testimonials</a>
						<a href="#">Effective Advertising Pointers</a>
						<a href="#">Advertise Advertise Advertise</a>
						<a href="#">Truck Side Advertising Isn T It Time</a>
						<a href="#">Direct Mail Advertising How I Made 47</a>
					</div>
				</div>

        <div class="blog-side-section">
          <h4>categories</h4>
          <div class="blog-categories-link">
            <a href="#">Beauty</a>
            <a href="#">Lifestyle</a>
            <a href="#">Music</a>
          </div>
        </div>

        <div class="blog-side-section">
          <h4>tags</h4>
          <div class="blog-side-tags">
            <a href="#" class="blog-item-tag">travel</a>
            <a href="#" class="blog-item-tag">life style</a>
            <a href="#" class="blog-item-tag">fashion</a>
            <a href="#" class="blog-item-tag">art</a>
            <a href="#" class="blog-item-tag">food</a>
            <a href="#" class="blog-item-tag">beauty</a>
            <a href="#" class="blog-item-tag">style</a>
          </div>
        </div>
			</div>

      <div class="col-6 col-lg-4 order-lg-1 my-2">
        <a href="#" class="blog-post-link--end">previous story</a>
      </div>
      <div class="col-6 col-lg-4 order-lg-3 text-right my-2">
        <a href="#" class="blog-post-link--end">next story</a>
      </div>
      <div class="col-lg-4 order-lg-2 text-center my-2">
        <p class="blog-post-link--end">share this article</p>
        <div class="row">
          <div class="offset-3 offset-sm-4 offset-lg-3 col-2 col-sm-1 col-lg-2">
            <i class="fa fa-facebook-f"></i>
          </div>
          <div class="col-2 col-sm-1 col-lg-2">
            <i class="fa fa-twitter"></i>
          </div>
          <div class="col-2 col-sm-1 col-lg-2">
            <i class="fa fa-pinterest-p"></i>
          </div>
        </div>
      </div>


		</div>

	</div>
</section>

